    "@types/express": "^4.17.1",
    "@types/mocha": "^10.0.1",
    "esbuild-register": "^3.5.0",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@colyseus/auth": "^0.15.0",
