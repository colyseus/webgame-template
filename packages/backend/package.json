{
  "private": true,
  "name": "my-app",
  "version": "1.0.0",
  "description": "npm init template for bootstrapping an empty Colyseus project",
  "main": "build/index.js",
  "engines": {
    "node": ">= 16.13.0"
  },
  "scripts": {
    "start": "tsx watch src/index.ts",
    "loadtest": "tsx loadtest/example.ts --room my_room",
